{
    "name": "phpdish/theme-installer",
    "description": "A composer installer for installing PHPDish themes.",
    "homepage": "https://www.phpdish.com",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Slince",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "PHPDish\\ThemeInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPDish\\ThemeInstaller\\Test\\": "tests/"
        }
    },
    "extra": {
        "class": "PHPDish\\ThemeInstaller\\ThemeInstallerPlugin"
    }
}
